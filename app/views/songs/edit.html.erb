<% if flash[:errors] %>
<% flash[:errors].each do |errors| %>
    <%= error %>
    <% end %>
    <% end%>




<% form_for @song do |f| %>
<%= f.text_field :title %>
<%= f.check_box :released %>
<%= f.number_field :release_year %>
<%= f.text_field :artist_name %>
<%= f.text_field :genre %>


<%= button_to "Delete", song_path(@song), method: :delete %>

<%= f.submit %>

<%end%>